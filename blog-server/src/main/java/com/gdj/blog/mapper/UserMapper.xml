<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gdj.blog.mapper.UserMapper">

    <select id="selectByUserName" resultType="com.gdj.blog.entity.User">
        select * from user where username = #{username}
    </select>
    <select id="selectUserPage" resultType="com.gdj.blog.entity.User">
        select * from user
    </select>
</mapper>
