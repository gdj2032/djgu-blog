<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gdj.blog.mapper.RouteMapper">

    <select id="pages" resultType="com.gdj.blog.entity.Route">
        select * from route
    </select>
    <select id="selectByName" resultType="com.gdj.blog.entity.Route">
        select *
        from route
        where name = #{name}
    </select>
    <select id="selectByPath" resultType="com.gdj.blog.entity.Route">
        select *
        from route
        where path = #{path}
    </select>
</mapper>