{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "downlevelIteration": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "outDir": "appjs",
    "rootDir": "appsrc",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "paths": {
      "*": ["node_modules/*"]
    },
    "types": ["@types/node"]
  },
  "exclude": [
    "node_modules",
    "src"
  ]
}